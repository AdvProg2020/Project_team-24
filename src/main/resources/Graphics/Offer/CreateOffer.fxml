<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Scene xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="A_Client.Graphics.Creates.CreatOffer">
    <AnchorPane prefHeight="745.0" prefWidth="2000">
        <Pane layoutX="587.0" layoutY="37.0" prefWidth="600.0">
            <TextField fx:id="start_time" alignment="CENTER" layoutX="98.0" layoutY="134.0" prefHeight="48.0" prefWidth="404.0" promptText="تاریخ آغاز  ( به صورت DD/MM/YYYY )" styleClass="startDate" >
                <font>
                    <Font size="18.0" />
                </font>
            </TextField>
            <TextField fx:id="end_time" alignment="CENTER" layoutX="99.0" layoutY="203.0" prefHeight="50.0" prefWidth="403.0" promptText="تاریخ پایان ( به صورت DD/MM/YYYY )" styleClass="endDate" >
                <font>
                    <Font size="18.0" />
                </font>
            </TextField>
            <Label alignment="CENTER" layoutX="146.0" layoutY="57.0" prefHeight="41.0" prefWidth="309.0" styleClass="title" text="صفحه ی ایجاد مزایده" textFill="#848484">
                <font>
                    <Font size="24.0" />
                </font>
            </Label>
            <Button fx:id="submit_btn" layoutX="222.0" layoutY="399.0" mnemonicParsing="false" onAction="#submit" prefHeight="40.0" prefWidth="156.0"  text="ثبت اطلاعات" textFill="#7e9a01">
                <font>
                    <Font size="18.0" />
                </font>
            </Button>

        </Pane>
    </AnchorPane></Scene>
