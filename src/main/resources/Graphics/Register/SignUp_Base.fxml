<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Scene xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphics.SignUp">
    <AnchorPane prefHeight="750.0" prefWidth="650.0" styleClass="background" stylesheets="@../Login/Login.css">

        <ImageView fitHeight="670.0" fitWidth="512.0" layoutX="63.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@PinClipart.com_square-frame-clipart_4974573.png" />
        </ImageView>

        <TextField fx:id="username" layoutX="221.0" layoutY="252.0" prefHeight="38.0" prefWidth="227.0" promptText="نام کاربری" />

        <PasswordField fx:id="passwordOne" layoutX="221.0" layoutY="300.0" prefHeight="38.0" prefWidth="227.0" promptText="رمز عبور" />
        <PasswordField fx:id="passwordTow" layoutX="221.0" layoutY="348.0" prefHeight="38.0" prefWidth="227.0" promptText="تکرار رمز عبور" />

        <CheckBox layoutX="224.0" layoutY="398.0" mnemonicParsing="false" text="مرا به خاطر بسپار" textFill="#797777" />

        <Button alignment="CENTER_RIGHT" layoutX="185.0" layoutY="541.0" mnemonicParsing="false" onAction="#goLogin" prefHeight="39.0" prefWidth="126.0" text="ورود" textFill="#2c80b0" />
        <Button alignment="CENTER_RIGHT" layoutX="248.0" layoutY="465.0" mnemonicParsing="false" onAction="#goNext" prefHeight="39.0" prefWidth="153.0" text="مرحله بعدی" textFill="#b71c1c">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="213.0" layoutY="194.0" prefHeight="27.0" prefWidth="227.0" text="ثبت نام" textFill="#717171">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label alignment="CENTER_RIGHT" layoutX="311.0" layoutY="550.0" prefHeight="26.0" prefWidth="137.0" text="سه سوتی هستید؟" textFill="#1d90ba" />
        <ImageView fitHeight="27.0" fitWidth="25.0" layoutX="186.0" layoutY="259.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icons/icons8-person-64.png" />
        </ImageView>
        <ImageView fitHeight="27.0" fitWidth="25.0" layoutX="186.0" layoutY="307.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icons/icons8-lock-26(2).png" />
        </ImageView>
        <ImageView fitHeight="27.0" fitWidth="25.0" layoutX="186.0" layoutY="355.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icons/icons8-lock-26(2).png" />
        </ImageView>
        <ImageView fitHeight="163.0" fitWidth="210.0" layoutX="213.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@logo.png" />
        </ImageView>
        <Separator layoutX="182.0" layoutY="524.0" prefHeight="3.0" prefWidth="286.0" />
        <Separator layoutX="185.0" layoutY="232.0" prefHeight="3.0" prefWidth="286.0" />
        <ImageView fitHeight="46.0" fitWidth="42.0" layoutX="218.0" layoutY="542.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icons/icons8-add-fingerprint-64.png" />
        </ImageView>
        <ImageView fitHeight="38.0" fitWidth="36.0" layoutX="249.0" layoutY="467.0" pickOnBounds="true" preserveRatio="true">
            <Image url="@../icons/icons8-add-user-group-man-man-50(1).png" />
        </ImageView>

        <Label fx:id="status" alignment="CENTER" layoutX="256.0" layoutY="436.0" prefHeight="18.0" prefWidth="137.0" text=";)" textAlignment="CENTER" />

        <Button layoutX="134.0" layoutY="633.0" mnemonicParsing="false" onAction="#goMainMenu" prefHeight="26.0" prefWidth="210.0" text="بازگشت به صفحه ی اصلی" />
        <ChoiceBox fx:id="chooseType" layoutX="340.0" layoutY="395.0" prefHeight="26.0" prefWidth="108.0" />

    </AnchorPane>
</Scene>
